{
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "cover": "jest --coverage",
    "test-watch": "jest --watch",
    "test:ci": "jest --ci",
    "lint": "eslint --fix --ext .ts,.tsx .",
    "prepare": "husky install",
    "clean": "rm *.tsbuildinfo || true",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook -s public",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test dev http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 cypress:headless",
    "docs": "jsdoc -c jsdoc.conf.json",
    "swagger-gen": "next-swagger-doc-cli swagger.config.json",
    "openapi:yaml": "typeconv -f ts -t oapi --oapi-format yaml --output-extension 'swagger.yaml' --output-directory 'models/openapi' 'models/*.ts'",
    "postbuild": "cpy 'models/**/*.swagger.yaml' public/openapi --flat",
    "update-openapi-client": "openapi-generator-cli generate -i http://localhost:8000/openapi -o shared/proxy --generator-name typescript-fetch --additional-properties=typescriptThreePlus=true"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --debug"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "tsc-files --noEmit"
    ],
    "*.{html,css,less,ejs}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.165.0",
    "@aws-sdk/client-s3": "^3.165.0",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.8.2",
    "@mui/styles": "^5.8.0",
    "@types/crypto-js": "^4.1.1",
    "@types/formidable": "^2.0.5",
    "ajv": "^8.11.0",
    "autoprefixer": "^10.4.7",
    "aws-sdk": "^2.1222.0",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "cookies-next": "^2.1.1",
    "crypto-js": "^4.1.1",
    "formidable": "^2.0.1",
    "jsesc": "^3.0.2",
    "jsonwebtoken": "^8.5.1",
    "next": "13.1.2",
    "next-swagger-doc": "^0.3.6",
    "postcss": "^8.4.14",
    "react": "^18.1.0",
    "react-cookie": "^4.1.1",
    "react-dom": "^18.1.0",
    "react-file-drop": "^3.1.5",
    "react-icons": "^4.4.0",
    "sharp": "^0.31.3",
    "styled-components": "^5.3.5",
    "swagger-ui-react": "^4.15.5",
    "tailwindcss": "^3.0.24"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.8.2",
    "@mui/types": "^7.1.3",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.2.0",
    "@testing-library/user-event": "14.2.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/react": "18.0.9",
    "@types/styled-components": "^5.1.25",
    "@types/swagger-ui-react": "^4.11.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "babel-loader": "^8.3.0",
    "better-docs": "^2.7.2",
    "clean-jsdoc-theme": "^4.2.2",
    "cypress": "10.3.1",
    "eslint": "^8.0.1",
    "eslint-config-next": "12.1.6",
    "eslint-config-standard-with-typescript": "^27.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-storybook": "^0.6.10",
    "eslint-plugin-testing-library": "^5.5.1",
    "husky": "^8.0.3",
    "jest": "28.1.0",
    "jest-environment-jsdom": "28.1.0",
    "jsdoc": "^4.0.0",
    "lint-staged": "^13.1.0",
    "next-router-mock": "^0.9.1-beta.0",
    "prettier": "^2.8.3",
    "start-server-and-test": "1.14.0",
    "tsc-files": "^1.1.3",
    "typescript": "*"
  }
}
