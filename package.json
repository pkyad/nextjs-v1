		"superjson": "^1.12.3",
		"swagger-ui-react": "^4.18.3",
		"tailwindcss": "^3.0.24",
		"uilib1": "file:.yalc/uilib1",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@babel/core": "^7.20.12",
		"@changesets/cli": "^2.26.0",
		"@commitlint/cli": "^17.4.1",
		"@commitlint/config-conventional": "^17.4.0",
		"@emotion/react": "^11.11.0",
		"@emotion/styled": "^11.11.0",
		"@inquirer/prompts": "^1.0.3",
		"@mui/material": "^5.8.2",
