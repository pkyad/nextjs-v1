{
	"name": "@pkyad/test1",
	"private": true,
	"version": "3.0.0",
	"scripts": {
		"postinstall": "yarn gen-client;yarn fetch-services",
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"test": "jest",
		"cover": "jest --coverage",
		"test-watch": "jest --watch",
		"test:ci": "jest --ci",
		"lint": "eslint --fix --ext .ts,.tsx .",
		"prepare": "husky install",
		"clean": "rm *.tsbuildinfo || true",
		"storybook": "storybook dev -p 6006 -s ../public --no-version-updates",
		"build-storybook": "storybook build -s public",
		"cypress": "cypress open",
		"cypress:headless": "cypress run",
		"e2e": "start-server-and-test dev http://localhost:3000 cypress",
		"e2e:headless": "start-server-and-test dev http://localhost:3000 cypress:headless",
		"docs": "jsdoc -c jsdoc.conf.json",
		"swagger-gen": "rm models/build/*;typeconv -f ts -t oapi --oapi-format yaml --output-extension 'swagger.yaml' --output-directory 'models/build' 'models/*.ts';next-swagger-doc-cli swagger.config.json",
		"update-openapi-service": "openapi-generator-cli generate --generator-name typescript-fetch --additional-properties=typescriptThreePlus=true",
		"update-openapi-client": "openapi-generator-cli generate -i public/swagger.json -o shared/http-client --generator-name typescript-fetch --additional-properties=typescriptThreePlus=true",
		"commitlint": "commitlint --edit",
		"changeset": "changeset",
		"gen-changelog": "changeset version",
		"gen-client": "yarn swagger-gen;tsx scripts/fix-swagger.ts;yarn update-openapi-client",
		"fetch-services": "tsx scripts/fetch-services.ts"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged --debug"
		}
	},
	"lint-staged": {
		"*.{ts,tsx}": [
			"prettier --write",
			"eslint --fix",
			"tsc-files --noEmit"
		],
		"*.{html,css,less,ejs}": [
			"prettier --write"
		]
	},
	"dependencies": {
		"@mui/material": "^5.8.2",
		"@mui/styles": "^5.8.0",
		"@pkyad/uilib1": "2.12.0",
		"@tanstack/react-query": "^4.29.5",
		"@trpc/client": "^10.25.1",
		"@trpc/next": "^10.25.1",
		"@trpc/react-query": "^10.25.1",
		"@trpc/server": "^10.25.1",
		"@types/crypto-js": "^4.1.1",
		"@types/formidable": "^2.0.5",
		"ajv": "^8.11.0",
		"autoprefixer": "^10.4.7",
		"axios": "^0.27.2",
		"classnames": "^2.3.1",
		"cookies-next": "^2.1.1",
		"crypto-js": "^4.1.1",
		"formidable": "^2.0.1",
		"jsesc": "^3.0.2",
		"jsonwebtoken": "^9.0.0",
		"next": "13.1.2",
		"next-swagger-doc": "^0.3.6",
		"postcss": "^8.4.14",
		"react": "^18.1.0",
		"react-cookie": "^4.1.1",
		"react-dom": "^18.1.0",
		"react-file-drop": "^3.1.5",
		"react-icons": "^4.4.0",
		"sharp": "^0.31.3",
